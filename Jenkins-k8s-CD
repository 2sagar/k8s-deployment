pipeline {
  agent {
  label 'label2'
  }

  stages {
    stage('Cloning Git') {
      steps {
        checkout scm
      }
	}
	
	 stage('Deploying App to Kubernetes') {
      steps {
        script {
          kubernetesDeploy(configs: "k8s-deployment-service-file.yaml", kubeconfigId: "k8s")
        }
      }
    }
	
  }
} 
